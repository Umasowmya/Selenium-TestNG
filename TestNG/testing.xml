<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Login">
    <test name="Web Login">
        <classes>
            <class name="WebLogin">
                <methods>
                    <include name="HomeLogin"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="Mobile Login">
        <classes>
            <class name="MobileLogin">
                <methods>
                    <exclude name="MobileAboutLogin"></exclude>
                </methods>
            </class>
            
        </classes>
    </test>
</suite>